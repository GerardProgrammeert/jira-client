{
    "name": "beezmaster/jira-client",
    "description": "Jira API client",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": ">=11.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.15",
        "rector/rector": "*",
        "squizlabs/php_codesniffer": "^3.9",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^1.12"
    },
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Beezmaster\\JiraClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gerard van Hattem",
            "email": "gwjvanhattem@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Beezmaster\\JiraClient\\JiraServiceProvider"
            ]
        }
    }
}
